{
  "name": "queuey",
  "version": "0.1.0",
  "description": "Background Job/Event Queue. Works with multiple transports such as Redis.",
  "main": "build/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "NODE_ENV=testing ava --verbose"
  },
  "keywords": [
    "queue",
    "redis",
    "jobs",
    "events",
    "background",
    "processing",
    "sqs"
  ],
  "author": "Connor Imrie <connorimrie@outlook.com>",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "node-uuid": "^1.4.8"
  },
  "ava": {
    "files": [
      "tests/**/*.test.js"
    ],
    "source": [
      "**/*.{js,jsx}",
      "!dist/**/*"
    ],
    "match": [
      "*"
    ],
    "concurrency": 4,
    "failFast": true,
    "tap": false,
    "powerAssert": false,
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "babel": "inherit"
  }
}
